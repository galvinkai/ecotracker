{
  "version": 2,
  "builds": [
    {
      "src": "ecotracker/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/ecotracker/dist/$1" }
  ],
  "github": {
    "silent": true,
    "enabled": true,
    "autoAlias": true
  }
}
